{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch API (HTTPS)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/DeveloperStore.Api/bin/Debug/net8.0/DeveloperStore.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/DeveloperStore.Api",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:5001;http://localhost:5000"
      }
    },
    {
      "name": "Launch API (HTTP)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/DeveloperStore.Api/bin/Debug/net8.0/DeveloperStore.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/DeveloperStore.Api",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:5079"
      }
    },
    {
      "name": "Attach to Process",
      "type": "coreclr",
      "request": "attach"
    }
  ]
}